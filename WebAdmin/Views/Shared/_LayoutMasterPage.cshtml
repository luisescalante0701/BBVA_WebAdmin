        <script src="~/lib/sweetalert/sweetalert2.min.js"></script>
        <script src="~/lib/Scripts/Alert.js"></script>

        <script>
            $(document).ready(function () {
                $(".loader-page").css({ visibility: "hidden", opacity: "0" })
            });

        </script>

        <style>
                    .modal {
                        display: none;
                        position: fixed;
                        z-index: 1000;
                        top: 0;
                        left: 0;
                        height: 100%;
                        width: 100%;
                        background: rgba( 255, 255, 255, .8 ) url('../../images/loading7.gif') 50% 50% no-repeat;
                    }

                    /* When the body has the loading class, we turn
            the scrollbar off with overflow:hidden */
                    body.loading .modal {
                        overflow: hidden;
                    }

                    /* Anytime the body has the loading class, our
            modal element will be visible */
                    body.loading .modal {
                        display: block;
                    }
        </style>


    </environment>

